---
import MetaTags from "../components/MetaTags.astro";

export interface Props {
	title: string;
}

const { meta = {} } = Astro.props;
---

<!DOCTYPE html>
<html lang="en" class="black-background">
	<head>
		<MetaTags {...meta} />
	</head>
	<body class="m-0 left-0 relative overflow-x-hidden">
		<slot />
	</body>
</html>
